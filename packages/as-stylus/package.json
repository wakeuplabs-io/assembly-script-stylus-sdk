{
  "name": "as-stylus",
  "version": "1.0.0",
  "description": "SDK to build AssemblyScript contracts for Stylus",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "test-sdk": "dist/cli/build.js"
  },
  "scripts": {
    "generate": "tsc && node dist/commands/generate/generate.js",
    "build": "tsc && node dist/commands/build/build.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "assemblyscript": "^0.27.35",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "keccak256": "^1.0.6",
    "ts-morph": "^25.0.1"
  }
}
